{
  "schemaVersion": 1,
  "id": "peculia",
  "version": "${version}",

  "name": "Peculia",
  "description": "A mod that adds many extra things into the world of minecraft to add more overall fun content!",
  "authors": [
    "LunarLuned",
    "Sydokiddo (Coding & Texture Help)",
    "Ambient_ (Texture & Recipe Help / Composer)",
    "ShockMicro (CoreShader creator)"
  ],
  "contact": {
    "homepage": "https://www.twitter.com/CosmaMods",
    "sources": "https://github.com/LunarLuned/Peculia"
  },

  "license": "do whatever",
  "icon": "assets/peculia/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.lunarluned.peculia.Peculia"
    ],
    "client": [
      "net.lunarluned.peculia.PeculiaClientMod"
    ],
    "modmenu": [
      "net.lunarluned.peculia.config.ModMenuCompatibility"
    ]
  },
  "mixins": [
    "peculia.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "1.19.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
